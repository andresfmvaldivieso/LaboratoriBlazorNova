                        <RadzenNumeric @bind-Value=@Model.PesEmp class="col-12" />
                    </RadzenCard>
                </ChildContent>
            </RadzenPanel>
        </div>
        <div class="col-md-6 col-sm-12">
            <RadzenPanel AllowCollapse="true" Class="rz-my-10 rz-mx-auto w-100">
