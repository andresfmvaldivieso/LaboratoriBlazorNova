                        <RadzenNumeric @bind-Value=@Model.SexEmp ReadOnly="true" class="col-1" />
                        <RadzenDropDown @bind-Value=@Model.SexEmp Data=@genero LoadData=@LoadDataGenero Count="@countGenero" AllowVirtualization="true" AllowClear="true"
                                        AllowFiltering="true" TextProperty="DesGen" ValueProperty="CodGen" class="col-10" />
                        <RadzenText TextStyle="TextStyle.Subtitle2" TagName="TagName.H3">Estado Civil:</RadzenText>
                        <RadzenNumeric @bind-Value=@Model.EstCiv ReadOnly="true" class="col-1" />
                        <RadzenDropDown @bind-Value=@codEstCivil Data=@estCivil LoadData=@LoadDataEstCivil Count="@countEstCivil" AllowVirtualization="true" AllowClear="true"
                                        AllowFiltering="true" TextProperty="DesEst" ValueProperty="CodEst" class="col-10" />
                    </RadzenCard>
                </ChildContent>
            </RadzenPanel>
        </div>
    </div>
    <div class="row">
