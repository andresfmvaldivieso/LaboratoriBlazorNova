@page "/aa"
@using Novasoft.Shared.Models
@inject IUserLoginServices userLoginServices
<h3>UserLoginview</h3>
<div>
    <div>
        <form>
            <div>
                <label for="email">Correo:</label>
                <input type="email" id="email" @bind="@userLogin.Email" />
            </div>
            <div>
                <label for="password">Contraseña:</label>
                <input type="password" id="password" @bind="@userLogin.Clave" />
            </div>
            <button type="submit" @onclick="HandleLogin">Iniciar sesión</button>
        </form>
    </div>
</div>
@code {
    private UserLogin userLogin { get; set; } = new ();
    private async Task HandleLogin()
    {
        await userLoginServices.GetLogin(userLogin);
        
    }
}
